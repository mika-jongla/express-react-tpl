{
  "name": "express-hybrid-tpl",
  "version": "0.0.1",
  "description": "Hybrid Express App Template with Gulp, Browserify and React",
  "scripts": {
    "start": "node server/server.js",
    "test": "jest tests/cli/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mika-jongla/express-hybrid-tpl.git"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "jest": {
    "scriptPreprocessor": "tests/cli/preprocessor.js",
    "unmockedModulePathPatterns": [
      "./node_modules/react"
    ]
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "colors": "^1.1.0",
    "gulp": "^3.8.11",
    "gulp-notify": "^2.2.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-util": "^3.0.4",
    "jest-cli": "^0.4.2",
    "lodash": "^3.3.1",
    "merge-stream": "^0.1.7",
    "pretty-hrtime": "~1.0.0",
    "reactify": "^0.17.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "^2.4.0"
  },
  "dependencies": {
    "body-parser": "^1.12.4",
    "classnames": "^2.1.0",
    "consolidate": "^0.13.1",
    "cookie-parser": "^1.3.5",
    "cookie-session": "^1.1.0",
    "express": "^4.12.4",
    "flux": "^2.0.3",
    "handlebars": "^3.0.3",
    "jade": "^1.10.0",
    "react": "^0.13.2",
    "react-classset": "0.0.2",
    "react-tools": "^0.13.3"
  }
}
